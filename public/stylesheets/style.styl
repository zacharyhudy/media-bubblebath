body
  padding: 0px
  min-width:1900px;
  height:100%
  font: 19px "Lucida Grande", Helvetica, Arial, sans-serif


a
  color: #00B7FF;

#bg-overlay
  position:fixed
  top:0
  left:0
  width:100%
  height:100%
  z-index:0
  opacity:0.7
  background:white
  background-size:cover
  
.nodeMap
  width:400px
  height:200px

#title
  position:fixed
  bottom:0px
  left:20px
  font-family:'Helvetica'
  font-size:7em
  color:#414143
  font-weight:100
  opacity:1
  z-index:0
  text-align:right
  margin-right:50px

ul
  list-style:none
  float:left
  padding-left:200px

  -webkit-animation-name: moveUp;
  -webkit-animation-iteration-count: 1;
  -webkit-animation-timing-function: ease-in-out;
  -webkit-animation-duration: 1s;

  animation-name: moveUp;
  animation-iteration-count: 1;
  animation-timing-function: ease-in-out;
  animation-duration: 1s;
  
.no-left
  padding-left:0
.bubble
  width:50px
  height:50px
  border:1px solid rgba(65,65,67,1)
  border-radius:50%
  display:block
  z-index:5
  margin:50px

  -webkit-animation-name: bounce;
  -webkit-animation-iteration-count: infinite;
  -webkit-animation-timing-function: ease-in-out;
  -webkit-animation-duration: 1s;

  animation-name: bounce;
  animation-iteration-count: infinite;
  animation-timing-function: ease-in-out;
  animation-duration: 1s;
  
  i
    position:absolute
    z-index:9
    left:32%
    top:32%
    color:black

.innerbubble
  width:50px
  height:50px
  left:0px
  top:0px
  z-index:10
  border-radius:50%
  //background:rgba(209,238,252,0.8)
  background:rgba(65,65,67,1)

  -webkit-transform:scale(0)
  transform:scale(0)

  -webkit-transition: all 0.2s 0 ease-in-out;
  -moz-transition: all 0.2s 0s ease-in-out;
  -ms-transition: all 0.2s 0 ease-in-out;
  -o-transition: all 0.2s 0 ease-in-out;
  transition: all 0.2s 0 ease-in-out;

.bubble:hover .innerbubble
  -webkit-transform:scale(0.9)
  transform:scale(0.9)

.bubble:hover i
  color:white

.bubble:hover .outerbubble
  opacity:1
  pointer-events:auto

.outerbubble
  position:absolute
  margin:0 auto
  background:white
  width:200px
  min-height:150px
  top:0px
  left:80px;
  opacity:0
  z-index:9999
  pointer-events:none
  border: 1px solid #DADFE1
  text-align:center

  -webkit-transition: all 0.5s 0 ease-in-out;
  -moz-transition: all 0.5s 0s ease-in-out;
  -ms-transition: all 0.5s 0 ease-in-out;
  -o-transition: all 0.5s 0 ease-in-out;
  transition: all 0.5s 0 ease-in-out;

.outerbubble img
  margin:0 auto
  width:150px
  padding:25px
  height:auto
.outerbubble p
  color:#6C7A89
  width:180px
  line-height:1em
  white-break:normal
  font-size:0.7em
  padding:10px
  padding-top:0px
  padding-bottom:2px



.form
  position:fixed
  top:90%
  right:20px
.search
  background:none
  width:500px
  height:30px
  border:none
  border-bottom:1px solid #414143
  border-radius:2px
  color:#414143
  font-size:1em
.search:focus
  outline:none
.button
  background:none
  width:100px
  min-height:20px
  color:#414143
  font-size:0.7em
  margin-left:20px
  border:none
  outline:none
.preloader 
  display: block;
  position: fixed;
  width: 100%;
  height: 100%;
  top:0;
  left:0;
  opacity:0;
  z-index: 9999;
  pointer-events:none
  text-align:center
  background: white;

  -webkit-transition: all 0.2s 0 ease-in-out;
  -moz-transition: all 0.2s 0s ease-in-out;
  -ms-transition: all 0.2s 0 ease-in-out;
  -o-transition: all 0.2s 0 ease-in-out;
  transition: all 0.2s 0 ease-in-out;

  img
    position:relative
    top:30%










@-webkit-keyframes moveUp {
    0% {
        -webkit-transform: translateY(100vh);
    }
    100% {
        -webkit-transform: translateY(0);
    }
}
@keyframes moveUp {
    0% {
        transform: translateY(100vh);
    }
    100% {
        transform: translateY(0);
    }
}

@-webkit-keyframes bounce {
    0% {
        -webkit-transform: translateY(10px);
    }
    50%{
        -webkit-transform: translateY(0);
    }
    100% {
        -webkit-transform: translateY(10px);
    }
}
@keyframes bounce {
    0% {
        -webkit-transform: translateY(10px);
    }
    50%{
        -webkit-transform: translateY(0);
    }
    100% {
        -webkit-transform: translateY(10px);
    }
}





